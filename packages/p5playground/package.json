{
  "name": "p5playground",
  "version": "0.0.1",
  "description": "Canvas games and sketches",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "dist/js/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist"
  },
  "license": "MIT",
  "keywords": [
    "p5js",
    "canvas",
    "games",
    "sketches"
  ],
  "author": "",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "npm-run-all clean build:*",
    "build:sketches": "webpack --config webpack.config.js --mode production",
    "clean": "rimraf dist",
    "dev": "npm-run-all build:sketches dev:*",
    "dev:sketches": "webpack-dev-server --config webpack.config.js --mode development --open"
  },
  "dependencies": {
    "@dimforge/rapier2d": "0.11.2",
    "canvas-sketch-util": "1.10.0",
    "p5": "1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tweakpane": "3.1.9"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-modules-commonjs": "7.21.2",
    "@babel/plugin-transform-strict-mode": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-flow": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@types/node": "18.11.17",
    "@types/p5": "1.6.1",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@webpack-cli/serve": "2.0.2",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.1",
    "mini-css-extract-plugin": "2.7.5",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.0",
    "style-loader": "3.3.2",
    "ts-loader": "9.4.2",
    "tsconfig": "workspace:*",
    "typescript": "4.9.4",
    "vite": "4.3.3",
    "webpack": "5.77.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.1",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  }
}
